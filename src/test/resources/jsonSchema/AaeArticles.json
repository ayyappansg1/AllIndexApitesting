                          "subTopicContentId": {
                            "title": "Subtopiccontentid",
                            "type": "string",
                            "default": "",
                            "pattern": "^.*$"
                          },
                          "subTopicName": {
                            "title": "Subtopicname",
                            "type": "string",
                            "default": "",
                            "pattern": "^.*$"
                          },
                          "subTopicContent": {
                            "title": "Subtopiccontent",
                            "type": ["null","string"],
                            "default": "",
                            "pattern": "^.*$"
                          },
                          "subTopicContentType": {
                            "title": "Subtopiccontenttype",
                            "type": "integer",
                            "default": 0
                          },
                          "subTopicContentTypeName": {
                            "title": "Subtopiccontenttypename",
                            "type": ["string","null"],
                            "default": null
                          },
                          "publishedDate": {
                            "title": "Publisheddate",
                            "type": "string",
                            "default": "",
                            "pattern": "^.*$"
                          },
                          "createdBy": {
                            "title": "Createdby",
                            "type": ["string","null"],
                            "default": null
                          },
