                        "type": "object",
                        "required": [
                          "subTopicId",
                          "subTopicContentId",
                          "subTopicName",
                          "subTopicContent",
                          "subTopicContentType",
                          "subTopicContentTypeName",
                          "publishedDate",
                          "createdBy",
                          "tags",
                          "imageUrl",
                          "tertiaryTopics"
