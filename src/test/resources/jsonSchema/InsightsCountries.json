{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Root",
  "type": "object",
  "required": [
    "responseCode",
    "message",
    "errors",
    "data"
  ],
  "properties": {
    "responseCode": {
      "title": "Responsecode",
      "type": "integer",
      "default": 0
    },
    "message": {
      "title": "Message",
      "type": "string",
      "default": "",
      "pattern": "^.*$"
    },
    "errors": {
      "title": "Errors",
      "type": "null",
      "default": null
    },
    "data": {
      "title": "Data",
      "type": "array",
      "default": [],
      "items":{
        "title": "Items",
        "type": "object",
        "required": [
          "id",
          "country_Name",
          "country_Code",
          "order_Id",
          "region_Id",
          "is_Deleted",
          "is_Active",
          "created_At",
          "updated_At",
          "flag_Upload_Id",
          "header_Image_Id",
          "haveSpecifics",
          "country_Created_At",
          "country_Updated_At",
          "videoLink",
          "isCompanyFavourite",
          "isUserFavourite",
          "headerImageUrl",
          "flagImageUrl"
        ],
        "properties": {
          "id": {
            "title": "Id",
            "type": "integer",
            "default": 0
          },
          "country_Name": {
            "title": "Country_name",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "country_Code": {
            "title": "Country_code",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "order_Id": {
            "title": "Order_id",
            "type": "integer",
            "default": 0
          },
          "region_Id": {
            "title": "Region_id",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "is_Deleted": {
            "title": "Is_deleted",
            "type": "boolean",
            "default": true
          },
          "is_Active": {
            "title": "Is_active",
            "type": "boolean",
            "default": true
          },
          "created_At": {
            "title": "Created_at",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "updated_At": {
            "title": "Updated_at",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "flag_Upload_Id": {
            "title": "Flag_upload_id",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "header_Image_Id": {
            "title": "Header_image_id",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "haveSpecifics": {
            "title": "Havespecifics",
            "type": "boolean",
            "default": true
          },
          "country_Created_At": {
            "title": "Country_created_at",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "country_Updated_At": {
            "title": "Country_updated_at",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "videoLink": {
            "title": "Videolink",
            "type": ["string","null"],
            "default": null
          },
          "isCompanyFavourite": {
            "title": "Iscompanyfavourite",
            "type": "boolean",
            "default": true
          },
          "isUserFavourite": {
            "title": "Isuserfavourite",
            "type": "boolean",
            "default": true
          },
          "headerImageUrl": {
            "title": "Headerimageurl",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          },
          "flagImageUrl": {
            "title": "Flagimageurl",
            "type": "string",
            "default": "",
            "pattern": "^.*$"
          }
        }
      }

    }
  }
}
