                  "title": "Items",
                  "type": "object",
                  "required": [
                    "topicId",
                    "topicContentId",
                    "topicName",
                    "topicContent",
                    "topicContentType",
                    "topicContentTypeName",
                    "publishedDate",
                    "createdBy",
                    "tags",
                    "imageUrl",
