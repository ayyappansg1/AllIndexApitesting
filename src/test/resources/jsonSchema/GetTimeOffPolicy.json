{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Root",
  "type": "object",
  "required": [
    "responseCode",
    "message",
    "errors",
    "data"
  ],
  "properties": {
    "responseCode": {
      "title": "Responsecode",
      "type": "integer",
      "default": 0
    },
    "message": {
      "title": "Message",
      "type": "string",
      "default": "",
      "pattern": "^.*$"
    },
    "errors": {
      "title": "Errors",
      "type": "null",
      "default": null
    },
    "data": {
      "title": "Data",
      "type": "object",
      "required": [
        "data",
        "pageNumber",
        "pageSize",
        "totalCount"
      ],
      "properties": {
        "data": {
          "title": "Data",
          "type": "array",
          "default": [],
          "items":{
            "title": "Items",
            "type": "object",
            "required": [
              "id",
              "policyName",
              "timeoffTypeId",
              "countryId",
              "country",
              "clientName",
              "policyTypeName",
              "effectiveStartDate",
              "effectiveEndDate",
              "statusId",
              "status",
              "createdAt",
              "customerIds",
              "addTimeOffPolicyPlanModel",
              "policyHolidayMappingDtos",
              "accrualPreferenceModel",
              "timeOfPolicyAssociated"
            ],
            "properties": {
              "id": {
                "title": "Id",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "policyName": {
                "title": "Policyname",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "timeoffTypeId": {
                "title": "Timeofftypeid",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "countryId": {
                "title": "Countryid",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "country": {
                "title": "Country",
                "type": "null",
                "default": null
              },
              "clientName": {
                "title": "Clientname",
                "type": ["null","string"],
                "default": null
              },
              "policyTypeName": {
                "title": "Policytypename",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "effectiveStartDate": {
                "title": "Effectivestartdate",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "effectiveEndDate": {
                "title": "Effectiveenddate",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "statusId": {
                "title": "Statusid",
                "type": "integer",
                "default": 0
              },
              "status": {
                "title": "Status",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "createdAt": {
                "title": "Createdat",
                "type": "string",
                "default": "",
                "pattern": "^.*$"
              },
              "customerIds": {
                "title": "Customerids",
                "type": "array",
                "default": [],
                "items":{
                  "title": "Items",
                  "type": "string",
                  "default": "",
                  "pattern": "^.*$"
                }
              },
              "addTimeOffPolicyPlanModel": {
                "title": "Addtimeoffpolicyplanmodel",
                "type": "null",
                "default": null
              },
              "policyHolidayMappingDtos": {
                "title": "Policyholidaymappingdtos",
                "type": "null",
                "default": null
              },
              "accrualPreferenceModel": {
                "title": "Accrualpreferencemodel",
                "type": "null",
                "default": null
              },
              "timeOfPolicyAssociated": {
                "title": "Timeofpolicyassociated",
                "type": "null",
                "default": null
              }
            }
          }

        },
        "pageNumber": {
          "title": "Pagenumber",
          "type": "integer",
          "default": 0
        },
        "pageSize": {
          "title": "Pagesize",
          "type": "integer",
          "default": 0
        },
        "totalCount": {
          "title": "Totalcount",
          "type": "integer",
          "default": 0
        }
      }
    }

  }
}
